    {
      "parameters": {
        "jsCode": "const summary = items\n  .map((item, i) => {\n    const { title, content, isoDate, pubDate, link } = item.json;\n\n    const date = isoDate || pubDate || \"\";\n\n    const shortDesc = content\n      ? content.length > 200\n        ? content.substring(0, 200) + \"...\"\n        : content\n      : \"\";\n\n    return `${i + 1}.\\n` +\n           `Title: ${title}\\n` +\n           `Description: ${shortDesc}\\n` +\n           `Date: ${date}\\n` +\n           `Link: ${link || \"\"}`;\n  })\n  .join('\\n\\n');\n\nreturn [{\n  json: {\n    summaryInput: summary\n  }\n}];\n"
      },
